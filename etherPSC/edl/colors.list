alarm  { 
  disconnected : "white/top"
  invalid      : "needs attention"
  minor        : "minor alarm"
  major        : "major alarm"
  noalarm      : "no alarm"
 }

rule 80 "advisor status" {
 >=-0.5 && <0.5	 : "no alarm"
 >=0.5  && <1.5	 : "minor alarm"
 >=1.5  && <=2.5 : "information"
 >2.5            : "major alarm"
}

rule 81 "BL status" {
 >=-0.5 && <0.5	 : "no alarm"
 >=0.5  && <1.5	 : "major alarm"
 >=1.5  && <=2.5 : "needs attention"
 >2.5            : "information"
}

rule 82 "BSOIC status" {
 >=-0.5 && <0.5	 : "major alarm"
 >=0.5  && <1.5	 : "no alarm"
 >=1.5  && <=2.5 : "minor alarm"
 >2.5            : "needs attention"
}

rule 83 "background/foreground" {
 >=-0.5 && <=0.5 : "background"
 >=0.5   	 : "foreground/bottom"
}

rule 84 "valve status" {
 >=-0.5 && <0.5	 : "minor alarm"
 >=0.5  && <=1.5 : "no alarm"
 >1.5            : "major alarm"
}

rule 85 "alarm/foreground" {
 >=-0.5 && <0.5	 : "foreground/bottom"
 >=0.5  && <1.5	 : "minor alarm"
 >=1.5  && <=2.5 : "major alarm"
 >2.5            : "needs attention"
}

rule 86 "alarm/background" {
 >=-0.5 && <0.5	 : "background"
 >=0.5  && <1.5	 : "minor alarm"
 >=1.5  && <=2.5 : "major alarm"
 >2.5            : "needs attention"
}

rule 87 "alarm/beige" {
 >=-0.5 && <0.5	 : "beige"
 >=0.5  && <1.5	 : "minor alarm"
 >=1.5  && <=2.5 : "major alarm"
 >2.5            : "needs attention"
}

rule 88 "foreground/background" {
 >=-0.5 && <=2.5 : "foreground/bottom"
 >=2.5   	     : "background"
}

rule 89 "bl12" {
 >=-0.5 && <=3.5 : "background"
 >=3.5   	     : "foreground/bottom"
}

rule 90 "pps-green" {
 >=0.5   	     : "green"
 >=-0.5 && <=0.5 : "foreground/bottom"
}

rule 91 "pps-yellow" {
 >=0.5   	     : "yellow"
 >=-0.5 && <=0.5 : "foreground/bottom"
}

rule 92 "pps-red" {
 >=0.5   	     : "red"
 >=-0.5 && <=0.5 : "foreground/bottom"
}

rule 93 "pps-black" {
 >=0.5   	     : "foreground/bottom"
 >=-0.5 && <=0.5 : "green"
}

